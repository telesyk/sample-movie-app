  "awardsHtml": "<div class=\"awards\">\r\n    <h5>29 wins & 23 nominations</h5>\r\n    <h3>Academy Awards, USA</h3>\r\n    <table>\r\n        <tbody>\r\n            <tr>\r\n                <td>\r\n                    <b>1996 Nominee Oscar</b>\r\n                    <br />\r\n                    Best Writing, Screenplay Written Directly for the Screen\r\n                    <br />\r\n                    Joss Whedon (screenplay) - Andrew Stanton (screenplay/story) - Joel Cohen (screenplay) - Alec Sokolow (screenplay) - John Lasseter (story) - Pete Docter (story) - Joe Ranft (story)\r\n                </td>\r\n            </tr>\r\n            <tr>\r\n                <td>\r\n                    <b>1996 Nominee Oscar</b>\r\n                    <br />\r\n                    Best Music, Original Song\r\n                    <br />\r\n                    Randy Newman (For the song \"You've Got a Friend in Me\".)\r\n                </td>\r\n            </tr>\r\n            <tr>\r\n                <td>\r\n                    <b>1996 Nominee Oscar</b>\r\n                    <br />\r\n                    Best Music, Original Musical or Comedy Score\r\n                    <br />\r\n                    Randy Newman\r\n                </td>\r\n            </tr>\r\n            <tr>\r\n                <td>\r\n                    <b>1996 Winner Special Achievement Award</b>\r\n                    <br />\r\n                    John Lasseter (For the development and inspired application of techniques that have made possible the first feature-length computer-animated film.)\r\n                </td>\r\n            </tr>\r\n        </tbody>\r\n    </table>\r\n    <h3>BAFTA Awards</h3>\r\n    <table>\r\n        <tbody>\r\n            <tr>\r\n                <td>\r\n                    <b>1997 Nominee BAFTA Film Award</b>\r\n                    <br />\r\n                    Best Achievement in Special Visual Effects\r\n                    <br />\r\n                    Eben Ostby - William R