  "awardsHtml": "<div class=\"awards\">\r\n    <h5>23 wins & 20 nominations</h5>\r\n    <h3>Academy Awards, USA</h3>\r\n    <table>\r\n        <tbody>\r\n            <tr>\r\n                <td>\r\n                    <b>1984 Nominee Oscar</b>\r\n                    <br />\r\n                    Best Art Direction-Set Decoration\r\n                    <br />\r\n                    Norman Reynolds - Fred Hole - James L. Schoppe - Michael Ford\r\n                </td>\r\n            </tr>\r\n            <tr>\r\n                <td>\r\n                    <b>1984 Nominee Oscar</b>\r\n                    <br />\r\n                    Best Sound\r\n                    <br />\r\n                    Ben Burtt - Gary Summers - Randy Thom - Tony Dawe\r\n                </td>\r\n            </tr>\r\n            <tr>\r\n                <td>\r\n                    <b>1984 Nominee Oscar</b>\r\n                    <br />\r\n                    Best Effects, Sound Effects Editing\r\n                    <br />\r\n                    Ben Burtt\r\n                </td>\r\n            </tr>\r\n            <tr>\r\n                <td>\r\n                    <b>1984 Nominee Oscar</b>\r\n                    <br />\r\n                    Best Music, Original Score\r\n                    <br />\r\n                    John Williams\r\n                </td>\r\n            </tr>\r\n            <tr>\r\n                <td>\r\n                    <b>1984 Winner Special Achievement Award</b>\r\n                    <br />\r\n                    Richard Edlund - Dennis Muren - Ken Ralston - Phil Tippett (For visual effects (statuette).)\r\n                </td>\r\n            </tr>\r\n        </tbody>\r\n    </table>\r\n    <h3>BAFTA Awards</h3>\r\n    <table>\r\n        <tbody>\r\n            <tr>\r\n                <td>\r\n                    <b>1984 Nominee BAFTA Film Award</b>\r\n                    <br />\r\n                    Best Make Up Artist\r\n                    <br />\r\n                    Phil Tippett - Stuart Freeborn\r\n                </td>\r\n            </tr>\r\n            <tr>\r\n                <td>\r\n                    <b>1984 Nominee BAFTA Film Award</b>\r\n                    <br />\r\n                    Best Production Design/Art Direction\r\n                    <br />\r\n                    Norman Reynolds\r\n                </td>\r\n            </tr>\r\n            <tr>\r\n                <td>\r\n                    <b>1984 Nominee BAFTA Film Award</b>\r\n                    <br />\r\n                    Best Sound\r\n                    <br />\r\n                    Ben Burtt - Tony Dawe - Gary Summers\r\n                </td>\r\n            </tr>\r\n            <tr>\r\n                <td>\r\n                    <b>1984 Winner BAFTA Film Award</b>\r\n                    <br />\r\n                    Best Special Visual Effects\r\n                    <br />\r\n                    Richard Edlund - Dennis Muren - Ken Ralston - Kit West\r\n                </td>\r\n            </tr>\r\n        </tbody>\r\n    </table>\r\n    <h3>National Film Preservation Board, USA</h3>\r\n    <table>\r\n        <tbody>\r\n            <tr>\r\n                <td>\r\n                    <b>2021 Winner National Film Registry</b>\r\n                    <br />\r\n                    National Film Preservation Board\r\n                </td>\r\n            </tr>\r\n        </tbody>\r\n    </table>\r\n    <h3>Online Film & Television Association</h3>\r\n    <table>\r\n        <tbody>\r\n            <tr>\r\n                <td>\r\n                    <b>2019 Winner OFTA Film Hall of Fame</b>\r\n                    <br />\r\n                    Motion Picture\r\n                </td>\r\n            </tr>\r\n        </tbody>\r\n    </table>\r\n    <h3>Guinness World Record Award</h3>\r\n    <table>\r\n        <tbody>\r\n            <tr>\r\n                <td>\r\n                    <b>2015 Winner Guinness World Record</b>\r\n                    <br />\r