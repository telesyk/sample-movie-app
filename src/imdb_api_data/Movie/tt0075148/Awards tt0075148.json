  "awardsHtml": "<div class=\"awards\">\r\n    <h5>21 wins & 23 nominations</h5>\r\n    <h3>Academy Awards, USA</h3>\r\n    <table>\r\n        <tbody>\r\n            <tr>\r\n                <td>\r\n                    <b>1977 Winner Oscar</b>\r\n                    <br />\r\n                    Best Picture\r\n                    <br />\r\n                    Irwin Winkler - Robert Chartoff\r\n                </td>\r\n            </tr>\r\n            <tr>\r\n                <td>\r\n                    <b>1977 Nominee Oscar</b>\r\n                    <br />\r\n                    Best Actor in a Leading Role\r\n                    <br />\r\n                    Sylvester Stallone (Sylvester Stallone becomes the third person to be nominated for both acting and writing in the same year, following Charles Chaplin for The Great Dictator (1940) and Orson Welles for Citizen Kane (1941).)\r\n                </td>\r\n            </tr>\r\n            <tr>\r\n                <td>\r\n                    <b>1977 Nominee Oscar</b>\r\n                    <br />\r\n                    Best Actress in a Leading Role\r\n                    <br />\r\n                    Talia Shire\r\n                </td>\r\n            </tr>\r\n            <tr>\r\n                <td>\r\n                    <b>1977 Nominee Oscar</b>\r\n                    <br />\r\n                    Best Actor in a Supporting Role\r\n                    <br />\r\n                    Burgess Meredith\r\n                </td>\r\n            </tr>\r\n            <tr>\r\n                <td>\r\n                    <b>1977 Nominee Oscar</b>\r\n                    <br />\r\n                    Best Actor in a Supporting Role\r\n                    <br />\r\n                    Burt Young\r\n                </td>\r\n            </tr>\r\n        </tbody>\r\n    </table>\r\n    <h3>BAFTA Awards</h3>\r\n    <table>\r\n        <tbody>\r\n            <tr>\r\n                <td>\r\n                    <b>1978 Nominee BAFTA Film Award</b>\r\n                    <br />\r\n                    Best Actor\r\n                    <br />\r\n                    Sylvester Stallone\r\n                </td>\r\n            </tr>\r\n            <tr>\r\n                <td>\r\n                    <b>1978 Nominee BAFTA Film Award</b>\r\n                    <br />\r\n                    Best Direction\r\n                    <br />\r\n                    John G. Avildsen\r\n                </td>\r\n            </tr>\r\n            <tr>\r\n                <td>\r\n                    <b>1978 Nominee BAFTA Film Award</b>\r\n                    <br />\r\n                    Best Editing\r\n                    <br />\r\n                    Richard Halsey\r\n                </td>\r\n            </tr>\r\n            <tr>\r\n                <td>\r\n                    <b>1978 Nominee BAFTA Film Award</b>\r\n                    <br />\r\n                    Best Film\r\n                </td>\r\n            </tr>\r\n            <tr>\r\n                <td>\r\n                    <b>1978 Nominee BAFTA Film Award</b>\r\n                    <br />\r\n                    Best Screenplay\r\n                    <br />\r\n                    Sylvester Stallone\r\n                </td>\r\n            </tr>\r\n        </tbody>\r\n    </table>\r\n    <h3>Online Film & Television Association</h3>\r\n    <table>\r\n        <tbody>\r\n            <tr>\r\n                <td>\r\n                    <b>2023 Winner OFTA Film Hall of Fame</b>\r\n                    <br />\r\n                    Character\r\n                    <br />\r\n                    Sylvester Stallone (as Rocky Balboa)\r\n                </td>\r\n            </tr>\r\n            <tr>\r\n                <td>\r\n                    <b>2018 Winner OFTA Film Hall of Fame</b>\r\n                    <br />\r\n                    Motion Picture\r\n                </td>\r\n            </tr>\r\n        </tbody>\r\n    </table>\r\n    <h3>Satellite Awards</h3>\r\n    